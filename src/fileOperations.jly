local readLines -> (path)
    local lines = {}
    for line in io.lines(path)
        table.insert(lines, line)
    end
    return lines
end

local writeFile -> (path, data, shebang)
    local f = io.open(path, "rb")
    if shebang
        f:write(shebang)
        f:write('\n')
    end
    f:write(data)
    f:close()
end